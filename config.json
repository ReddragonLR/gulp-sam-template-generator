{
    "APIResourceTemplate":{
    "<ResourceName>":
    {
        "Type": "AWS::Serverless::Function",
        "Properties": 
        {
            "Handler": "<ResourceFileName>.handler",
            "Events":
            {
                "<ResourceFileName>":
                {
                    "Type": "Api",
                    "Properties":
                    {
                        "Path": "/<ResourceName>",
                        "Method": "<HttpVerb>"
                    }
                }
            }
        }
    }
  },
    "AWSMainTemplate": 
    {
        "AWSTemplateFormatVersion": "2010-09-09",
        "Transform": "AWS::Serverless-2016-10-31",
        "Description": "SAM template generated by Gulp",
        "Globals":
        {
            "Function":
            {
                "MemorySize": 128,
                "Runtime": "nodejs8.10",
                "Timeout": 5
            }
        },
        "Resources":[]
    }
}